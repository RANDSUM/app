{
  "name": "randsumapp",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npm run gen:db:types && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export -p web",
    "deploy:web": "npm run build:web && netlify deploy --dir dist",
    "ts:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "gen:db:types": "supabase gen types typescript --project-id muaohwswudsfjzrjollg > ./database.types.ts"
  },
  "dependencies": {
    "@pchmn/expo-material3-theme": "^1.3.2",
    "expo": "^50.0.15",
    "expo-dev-client": "~3.3.11",
    "expo-router": "~3.4.8",
    "expo-status-bar": "~1.11.1",
    "randsum": "^3.2.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^6.1.2",
    "prettier": "3.2.5",
    "typescript": "^5.1.3"
  },
  "private": true
}
