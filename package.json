    "expo-router": "~3.4.8",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.12",
    "randsum": "^3.2.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-circular-progress-indicator": "^4.4.2",
    "react-native-collapsible": "^1.6.1",
