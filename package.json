  "scripts": {
    "start": "npm run gen:db:types && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export -p web",
    "deploy:web": "npm run build:web && netlify deploy --dir dist",
    "ts:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
